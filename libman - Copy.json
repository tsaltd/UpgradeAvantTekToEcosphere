{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/js/FlexBox/jquery/",
      "files": [
        "jquery.slim.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/js/FlexBox/popper.js/",
      "files": [
        "popper.min.js",
        "umd/popper.min.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css"
      ]
    }
  ]
}